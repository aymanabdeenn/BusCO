<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>BusCoProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list><!--Travel types-->
  <context-param>
  	<param-name>travelTypeCity</param-name>
  	<param-value>5</param-value>
  </context-param>
  <context-param>
  	<param-name>travelTypeInnerCity</param-name>
  	<param-value>2</param-value>
  </context-param><!--Ticket types--><context-param>
  	<param-name>oneTrip</param-name>
  	<param-value>1</param-value>
</context-param>
	<context-param>
		<param-name>dailyPass</param-name>
		<param-value>2</param-value>
	</context-param>
	<context-param>
		<param-name>weeklyPass</param-name>
		<param-value>3</param-value>
	</context-param>
	<context-param>
		<param-name>monthlyPass</param-name>
		<param-value>4</param-value>
	</context-param><!--Fare catagories--><context-param>
		<param-name>regular</param-name>
		<param-value>0</param-value>
</context-param>
	<context-param>
		<param-name>student</param-name>
		<param-value>20</param-value>
	</context-param>
	<context-param>
		<param-name>senior</param-name>
		<param-value>30</param-value>
	</context-param>
	<context-param>
		<param-name>eveningFare</param-name>
		<param-value>15</param-value>
	</context-param>
</web-app>